if [ "$TERM" = linux ] && command -v ttyscheme >/dev/null; then
	ttyscheme "gruvbox_dark"
fi
#[[ -f ~/.bashrc ]] && . ~/.bashrc

if uwsm check may-start; then
	exec uwsm start hyprland-uwsm.desktop
fi

